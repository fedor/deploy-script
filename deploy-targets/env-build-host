NODEJS_VERSION=v18.17.1
APP=app-example
APP_USER=$APP-$ENV
SYSTEMD_SOCKET_APP=$APP_USER.socket
SYSTEMD_SOCKETS="$SYSTEMD_SOCKET_APP"
SYSTEMD_SERVICE_APP=$APP_USER.service
SYSTEMD_SERVICE_MEMCACHED=docker.memcached.$APP_USER.service
SYSTEMD_SERVICE_POSTGRESQL=docker.postgresql.$APP_USER.service
SYSTEMD_SERVICE_MARIADB=docker.mariadb.$APP_USER.service
SYSTEMD_SERVICE_WORDPRESS=docker.wordpress.$APP_USER.service
SYSTEMD_SERVICE_ADMINER=docker.adminer.$APP_USER.service
SYSTEMD_SERVICES="$SYSTEMD_SERVICE_APP $SYSTEMD_SERVICE_MEMCACHED $SYSTEMD_SERVICE_POSTGRESQL $SYSTEMD_SERVICE_MARIADB $SYSTEMD_SERVICE_WORDPRESS $SYSTEMD_SERVICE_ADMINER"