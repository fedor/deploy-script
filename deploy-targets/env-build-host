NODEJS_VERSION=v18.17.1
APP=app-example
APP_USER=$APP-$ENV
SYSTEMD_APP_SOCKET=$APP_USER.socket
SYSTEMD_SOCKETS="$SYSTEMD_APP_SOCKET"
SYSTEMD_APP_SERVICE=$APP_USER.service
SYSTEMD_MEMCACHED_SERVICE=docker.memcached.$APP_USER.service
SYSTEMD_POSTGRESQL_SERVICE=docker.postgresql.$APP_USER.service
SYSTEMD_MARIADB_SERVICE=docker.mariadb.$APP_USER.service
SYSTEMD_WORDPRESS_SERVICE=docker.wordpress.$APP_USER.service
SYSTEMD_ADMINER_SERVICE=docker.adminer.$APP_USER.service
SYSTEMD_SERVICES="$SYSTEMD_APP_SERVICE $SYSTEMD_MEMCACHED_SERVICE $SYSTEMD_POSTGRESQL_SERVICE $SYSTEMD_MARIADB_SERVICE $SYSTEMD_WORDPRESS_SERVICE $SYSTEMD_ADMINER_SERVICE"